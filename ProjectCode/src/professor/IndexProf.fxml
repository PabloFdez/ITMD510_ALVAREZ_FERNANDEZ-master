<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="480.0" prefWidth="640.0" styleClass="pane2" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="professor.IndexProfController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="System">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#TKClose" text="Close Program" />
              <MenuItem mnemonicParsing="false" onAction="#TKSession" text="Close Session" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#TKHelp" text="General" />
                  <MenuItem mnemonicParsing="false" onAction="#TKSpecificHelp" text="Now" />
              <MenuItem mnemonicParsing="false" onAction="#TKAboutUs" text="About Us" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <left>
      <VBox prefHeight="451.0" prefWidth="93.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="143.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="file:./pics/professor.jpg" />
               </image>
            </ImageView>
            <Button onAction="#ChangingPaneProfArea" prefHeight="57.0" prefWidth="152.0" text="Professor information">
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
            <Button onAction="#ChangingPaneSetGrade" prefHeight="57.0" prefWidth="152.0" text="Set Grade">
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
         </children>
         <effect>
            <InnerShadow />
         </effect>
      </VBox>
   </left>
   <center>
      <Pane fx:id="ChangingPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
